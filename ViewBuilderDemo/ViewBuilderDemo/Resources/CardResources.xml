<?xml version="1.1" encoding="UTF-8"?>
<d:Content xmlns:d="@framework" xmlns="com.apple.UIKit" xmlns:m="@document">
    <d:Resources>
        <d:Item d:name="font" name="font" value="d:Font(name: AvenirNext-Regular, size:14)"/>
        <d:Item name="font2" value="d:Font(name: AvenirNext-UltraLight, size:14)"/>
        <d:Item d:name="font3" name="font3" value="d:Font(name: AvenirNext-Bold, size:14)"/>
        <d:Item name="bigFont" value="d:Font(name: AvenirNext-Bold, size:30)"/>
        <d:Item d:name="mediumFont" name="mediumFont" value="d:Font(name: AvenirNext-Bold, size:22)"/>
        <d:Item d:name="mediumFont2" name="mediumFont2" value="d:Font(name: AvenirNext-Regular, size:17)"/>
        <d:Item d:name="tinyFont" name="tinyFont" value="d:Font(name: AvenirNext-Regular, size:10)"/>
        <d:Item name="consoleFont" value="d:Font(name: Courier, size:14)"/>

        <d:Item name="color" value="d:Color(#FEFEFE)"/>
        <d:Item name="borderColor" value="d:Color(#DDDDDD)"/>
        <d:Item name="backgroundColor" value="d:Color(#EBEBF2)"/>
        <d:Item name="themeColor" value="d:Color(#0077B5)"/>
        <d:Item name="themeColorDark" value="d:Color(#006A9E)"/>

        <d:Item name="summary" value="Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s."/>
        <d:Item name="insight" value="It is a fact that a reader will be distracted by the readable content when looking its layout."/>

        <d:Item name="cellStyle">
            <d:ObjectStyle>
                <d:Setter property="cornerRadius" value="3"/>

                <d:Setter property="borderWidth" value="1"/>
                <d:Setter property="borderColor" value="d:Color(#DDDDDD)"/>

                <!--
                 <d:Setter property="shadowRadius" value="1"/>
                 <d:Setter property="shadowColor" value="d:Color(Gray)"/>
                 <d:Setter property="shadowOpacity" value="0.5"/>
                 <d:Setter property="shadowOffset" value="d:Size(0, 0)"/>
                 -->

            </d:ObjectStyle>
        </d:Item>
        
        <d:Item name="buttonStyle">
            <d:ObjectStyle>
                <d:Setter property="cornerRadius" value="2"/>
                <d:Setter property="borderWidth" value="1"/>
                <d:Setter property="borderColor" value="d:Color(#066CFF)"/>
                <d:Setter property="backgroundColor" value="d:Color(Clear)"/>
                <d:Setter property="contentEdgeInsets" value="d:EdgeInsets(2, 8)"/>

                <d:Setter property="titleColor" value="d:ColorForState(value: d:Color(#066CFF), state: d:ControlState(Normal))"/>
                <d:Setter property="titleColor" value="d:ColorForState(value: d:Color(#066CFF), state: d:ControlState(Highlighted))"/>

                <d:Setter property="font" value="@font3"/>
            </d:ObjectStyle>
        </d:Item>

        <d:Item name="buttonStyleWhite">
            <d:ObjectStyle>
                <d:Setter property="cornerRadius" value="2"/>
                <d:Setter property="borderWidth" value="1"/>
                <d:Setter property="borderColor" value="d:Color(White)"/>
                <d:Setter property="backgroundColor" value="d:Color(Clear)"/>
                <d:Setter property="contentEdgeInsets" value="d:EdgeInsets(2, 8)"/>

                <d:Setter property="titleColor" value="d:ColorForState(value: d:Color(White), state: d:ControlState(Normal))"/>
                <d:Setter property="titleColor" value="d:ColorForState(value: d:Color(White), state: d:ControlState(Highlighted))"/>

                <d:Setter property="font" value="@mediumFont"/>
            </d:ObjectStyle>
        </d:Item>

        <d:Item name="buttonStyleSmall">
            <d:ObjectStyle>
                <d:Setter property="cornerRadius" value="2"/>
                <d:Setter property="borderWidth" value="1"/>
                <d:Setter property="borderColor" value="d:Color(White)"/>
                <d:Setter property="backgroundColor" value="d:Color(Clear)"/>
                <d:Setter property="contentEdgeInsets" value="d:EdgeInsets(2, 8)"/>

                <d:Setter property="titleColor" value="d:ColorForState(value: d:Color(White), state: d:ControlState(Normal))"/>
                <d:Setter property="titleColor" value="d:ColorForState(value: d:Color(White), state: d:ControlState(Highlighted))"/>

                <d:Setter property="font" value="@tinyFont"/>
            </d:ObjectStyle>
        </d:Item>
    </d:Resources>
</d:Content>
