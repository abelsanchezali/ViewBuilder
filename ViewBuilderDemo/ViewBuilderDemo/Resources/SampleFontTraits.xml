<?xml version="1.1" encoding="UTF-8"?>
<d:Content xmlns:d="@framework" xmlns="com.apple.UIKit" xmlns:m="@document">
    <d:Resources>
        <d:Item name="backColor" value="d:Color(Gray)"/>
        <d:Item name="textColor" value="d:Color(Green)"/>
    </d:Resources>
    <UIScrollView bounds="d:Rect(0,0,300,300)" contentSize="d:Size(300,900)">
        <UIView frame="d:Rect(0,0,300,900)">
            <UILabel frame="d:Rect(0,  0,300,45)" text="Bold Bold Bold" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Helvetica Neue, size:20 traits: Bold)"/>
            <UILabel frame="d:Rect(0, 50,300,45)" text="Italic Italic Italic" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Helvetica Neue, size:20 traits: Italic)"/>
            <UILabel frame="d:Rect(0,100,300,45)" text="Expanded Expanded Expanded" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Helvetica Neue, size:20 traits: Expanded)"/>
            <UILabel frame="d:Rect(0,150,300,45)" text="Condensed Condensed Condensed" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Helvetica Neue, size:20 traits: Condensed)"/>
            <UILabel frame="d:Rect(0,200,300,45)" text="MonoSpace MonoSpace MonoSpace" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Helvetica Neue, size:20 traits: MonoSpace)"/>
            <UILabel frame="d:Rect(0,250,300,45)" text="Vertical Vertical Vertical" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Helvetica Neue, size:20 traits: Vertical)"/>
            <UILabel frame="d:Rect(0,300,300,45)" text="Italic Bold Italic Bold" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Helvetica Neue, size:20 traits: Italic, Bold)"/>
            <UILabel frame="d:Rect(0,350,300,45)" text="Optimized Optimized Optimized" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Helvetica Neue, size:20 traits: Optimized)"/>
            <UILabel frame="d:Rect(0,400,300,45)" text="Tight Tight Tight" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Helvetica Neue, size:20 traits: Tight)"/>
            <UILabel frame="d:Rect(0,450,300,45)" text="Loose Loose Loose" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Helvetica Neue, size:20 traits: Loose)"/>
            <UILabel frame="d:Rect(0,500,300,45)" text="HelveticaNeue-UltraLight" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: HelveticaNeue-UltraLight, size:20)"/>
            <UILabel frame="d:Rect(0,550,300,45)" text="GurmukhiMN-Bold" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: GurmukhiMN-Bold, size:20)"/>
            <UILabel frame="d:Rect(0,600,300,45)" text="GillSans-UltraBold" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: GillSans-UltraBold, size:20)"/>
            <UILabel frame="d:Rect(0,650,300,45)" text="MarkerFelt-Wide" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: MarkerFelt-Wide, size:20)"/>
            <UILabel frame="d:Rect(0,700,300,45)" text="TimesNewRomanPS-BoldItalicMT" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: TimesNewRomanPS-BoldItalicMT, size:20)"/>
            <UILabel frame="d:Rect(0,750,300,45)" text="Zapfino" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Zapfino, size:20)"/>
            <UILabel frame="d:Rect(0,800,300,45)" text="Futura-CondensedExtraBold" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Futura-CondensedExtraBold, size:20)"/>
            <UILabel frame="d:Rect(0,850,300,45)" text="Bodoni Ornaments" backgroundColor="@backColor" textColor="@textColor"
            font="d:Font(name: Bodoni Ornaments, size:20)"/>
        </UIView>
    </UIScrollView>
</d:Content>

